{
  "name": "panam-screensaver",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "docker:build": "docker build -t panam-screensaver:$npm_package_version .",
    "docker:run": "docker run -p 3000:3000 panam-screensaver",
    "docker:tag": "docker tag panam-screensaver:$npm_package_version docker.hostag.fr/panam-screensaver:$npm_package_version",
    "docker:push": "docker push docker.hostag.fr/panam-screensaver:$npm_package_version",
    "fetch": "bun run idfmFetch.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "5.13.0",
    "@sveltejs/adapter-node": "^5.0.1",
    "dotenv": "^16.4.5",
    "prisma": "^5.13.0",
    "tslog": "^4.9.2"
  }
}
